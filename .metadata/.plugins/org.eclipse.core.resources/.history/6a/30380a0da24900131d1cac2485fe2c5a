package cap5;

/**
 * @author raphael
 *
 */
public class TesteOperadorInstanceOf {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String nome = "";
		if (nome instanceof String)
			System.out.println("nome e String");
		else
			System.out.println("nome nao e String");
	}

}
